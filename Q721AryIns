class Main {
                  [0][1][2]
     //            1  3  4  7  9  11
     //                 [3][4][5]         2       99
  static void aryIns(int[] a, int idx, int x) {
    if (idx >= 0 && idx < a.length) {
      for (int i = a.length-1; i > idx; i--)
        a[i] = a[i-1];
      a[idx] = x;
    }
  }

  // main 은 생략 ( 7-19 와 동일)
  public static void main(String[] args) {
    System.out.println("Hello world!");
  }
}
